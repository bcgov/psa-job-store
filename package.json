    "@changesets/cli": "^2.27.10",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "prettier-plugin-packagejson": "^2.5.6",
    "tsconfig": "^7.0.0",
    "turbo": "^2.3.3"
  },
  "packageManager": "npm@10.9.1",
  "engines": {
    "node": ">=22.11.0 <23.0.0"
