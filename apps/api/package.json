{
  "name": "api",
  "version": "0.12.1",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-generated": "eslint \"src/@generated/**/*.ts\" --fix",
    "migrate:reset:e2e-test": "cross-env SEED_FILE=prisma/e2e-test-data-seed.ts npx prisma migrate reset",
    "run:script": "node dist/migrations/add-email.js",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start:repl": "nest start --entryFile repl --watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@bcgov/citz-imb-sso-css-api": "^1.0.0",
    "@elastic/elasticsearch": "^8.10.0",
    "@jordanwalsh23/postman-local-mock-server": "^0.2.4",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.9",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.9",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/schedule": "^4.0.0",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.2",
    "axios-oauth-client": "^2.2.0",
    "cache-manager": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "common-kit": "*",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.10",
    "diff-match-patch": "^1.0.5",
    "docx": "^8.5.0",
    "express": "^4.18.2",
    "fast-array-diff": "^1.1.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "prisma-graphql-type-decimal": "^3.0.0",
    "qs": "^6.12.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.9",
    "@types/diff-match-patch": "^1.0.36",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.3.1",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "eslint-config-custom": "^0.0.0",
    "jest": "^29.7.0",
    "nestjs-pino": "^3.5.0",
