    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig": "^0.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
